; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SQUIBBLE RACE"
run/main_scene="res://scenes/main_scene.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://icon.svg"

[autoload]

random="*res://scripts/random.gd"
info="res://scripts/runner_information.gd"
ref="*res://scripts/refs.gd"
runner="*res://runner/runner_info.tscn"
settings="*res://art/settings.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/initial_position_type=0
window/size/initial_position=Vector2i(2995, 0)
window/size/resizable=false
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[rendering]

renderer/rendering_method="mobile"
